(def fib
    (lambda (n)
   	(def nums (list 0 1))
	(if (= 1 n) (set nums (list 0))
	
	(def c 2)	

        (while (<= c n)
            (append nums (+ (index nums (- c 1)) (index nums (- c 2))))
            (set c (+ c 1))
        )

        nums
    )
)
        
(put (fib 35))
